{
    "manifest_version": 2,
    "name": "TeamInfo",
    "version": "1.0",
    "description": "Know more about teams!",


    "background": {
        "scripts": ["event.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "imgs/dota_logo.png",
        "default_popup": "popup.html"
    },

    "permissions": [
        "http://*/*",
        "https://*/*",
        "contextMenus",
        "tabs"
    ]
}
