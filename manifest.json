{
    "manifest_version": 2,
    "name": "TeamInfo",
    "version": "1.0",
    "description": "Know more about teams!",
    
    
    "background": {
        "scripts": ["event.js"],
        "persistent": false
    },
    "browser_action": {
        // "default_icon": {                    
        //     "19": "img/icon48.png",          
        //     "38": "img/icon48.png"          
        // },
        "default_popup": "popup.html"
    },
        
    "permissions": [
        "http://*/*",
        "https://*/*",
        "contextMenus",
        "tabs"
    ]
}